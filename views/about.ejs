    <%- include layout/navbar %>
    <div class="about">
      <img
        class="other_banner"
        src="/public/images/others_banner.jpg"
        alt="banner_img"
      />
      <div>
        <div class="sub_category">
          <ul>
            <li class="category_header">Introduction</li>
            <li class="category_item">
              <a href="/contact"> Contact&nbsp;Ways </a>
            </li>
            <li class="category_item">
